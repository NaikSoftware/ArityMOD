<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ua.naiksoftware.aritymod"
    android:versionCode="33"
    android:versionName="1.42">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="ArityMOD"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowGraph" />
        <!-- activity android:name=".ShowGraph3d" / -->
        <activity
            android:name=".HelpActivity"
            android:label="Arity Calculator" />
        <activity
            android:name=".Settings"
            android:label="Calculator settings" />
        <activity android:name=".DefsActivity"/>
    </application>

</manifest>
