<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="ua.naiksoftware.aritymod"
    android:versionCode="31"
    android:versionName="1.4">
    
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    
    <application android:label="ArityMOD" android:icon="@drawable/icon" android:allowBackup="true">
        <activity android:name=".MainActivity"
              android:label="@string/app_name"
              android:configChanges="orientation|keyboard|keyboardHidden"
        >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowGraph" />
        <!--activity android:name=".ShowGraph3d" /-->
        <activity android:name=".ListDefs" android:label="@string/user_defs" />
        <activity android:name=".Help" android:label="Arity Calculator" />
        <activity android:name=".Settings" android:label="Calculator settings" />
    </application>
</manifest> 
